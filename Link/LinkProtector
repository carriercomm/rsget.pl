name: LinkProtector
short: L:LinkProtector
uri: qr{link-protector\.com/}
slots: max
status: OK 2009-10-24

start:
	GET( $-{_uri} );
	! m#<INPUT type="button" value="CONTINUE ..." name="Continue" onClick="window.location='(.*?)'"#;

	LINK( $1 );

